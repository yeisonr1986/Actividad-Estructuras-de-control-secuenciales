
	Algoritmo ejercicio_6_Porcentaje_De_HyM
		//entrada 
		Escribir "numero de mujeres:"
		Leer mujeres
		Escribir "numero de hombres:"
		Leer hombres 
		// caja negra
		total_estudiantes<- (+mujeres+hombres)
		pormujeres<-(mujeres/total_estudiantes*100)
		porhombres<-(hombres/total_estudiantes*100)
		
		//salida
		Escribir "porcentaje de hombres",porhombres,"%"
		Escribir "porcentaje de mujeres",pormujeres,"%"
FinAlgoritmo
